      <Query>
        <DataSourceName>MARP01</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

SELECT 
	[Date] = CAST(signupdt AS DATE)
	,TotalRegs = COUNT(1)
	,MLBRegs = SUM(CASE WHEN brandId IN (1026932, 1026933, 1026934, 1026935, 1026936, 1026937, 1026938, 1026939, 1026940, 1026941, 1026942, 1026943, 1026944, 1026945, 1026946, 1026947, 1026948, 1026949, 1026950, 1026951, 1026952, 1026953, 1026954, 1026955, 1026956, 1026957, 1026958, 1026959, 1026960, 1026961, 1027006, 1026926) THEN 1 ELSE 0 END)
	,MLB_Per = (CAST(SUM(CASE WHEN brandId IN (1026932, 1026933, 1026934, 1026935, 1026936, 1026937, 1026938, 1026939, 1026940, 1026941, 1026942, 1026943, 1026944, 1026945, 1026946, 1026947, 1026948, 1026949, 1026950, 1026951, 1026952, 1026953, 1026954, 1026955, 1026956, 1026957, 1026958, 1026959, 1026960, 1026961, 1027006, 1026926) THEN 1 ELSE 0 END) AS DECIMAL) / COUNT(1)) * 100
FROM profilereaddata.dbo.users WITH (NOLOCK)
WHERE signupdt &gt;= '2014-03-01'
GROUP BY 
	CAST(signupdt AS DATE)
ORDER BY 
	CAST(signupdt AS DATE)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
