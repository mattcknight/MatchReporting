  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Debug</DataSourceName>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
SET NOCOUNT ON

DECLARE @QueryDate DATE
SELECT @QueryDate = GETDATE()

IF OBJECT_ID('tempdb..#AllDebugs') IS NOT NULL DROP TABLE #AllDebugs
IF OBJECT_ID('tempdb..#Debugs') IS NOT NULL DROP TABLE #Debugs

SELECT CAST(m.LogEntryDateTime AS DATE) AS MetricDate,m.CodebaseID,Debugs = COUNT(1)
INTO #AllDebugs
FROM AppDebug.dbo.MessageLog m WITH (NOLOCK)
WHERE m.LogEntryDateTime BETWEEN DATEADD(DAY, -10, @QueryDate) AND @QueryDate
GROUP BY CAST(LogEntryDateTime AS DATE),m.CodebaseID

SELECT
	t.CodebaseID
	,Debugs_Today = t.Debugs
	,Debugs_Avg = ad.AvgDebugs_All
	,ThresholdFactor = CONVERT(DECIMAL(10,2),ABS(t.Debugs - ISNULL(ad.AvgDebugs_All, 0)) / ad.AvgDebugs_All)
INTO #Debugs
FROM #AllDebugs t
LEFT JOIN (
    SELECT 
		CodebaseID
		,AvgDebugs_All = SUM(Debugs) / COUNT(DISTINCT MetricDate)
    FROM #AllDebugs
    WHERE MetricDate &lt; DATEADD(DAY,-1,@QueryDate)
    GROUP BY CodebaseID
    ) AS ad ON t.CodebaseID = ad.CodebaseID
WHERE t.MetricDate = DATEADD(DAY,-1,@QueryDate)
AND t.Debugs &gt; 10000
AND ( ISNULL(ad.AvgDebugs_All, 0) = 0 OR ABS(t.Debugs - ISNULL(ad.AvgDebugs_All, 0)) / ad.AvgDebugs_All &gt; 0.25 )
ORDER BY 4 DESC

SELECT 
	MetricDate
	,Codebase = acb.CodeBaseName
	,MetricValue = SUM(ad.Debugs)
FROM #AllDebugs ad
JOIN #Debugs d ON ad.CodebaseID = d.CodebaseID
JOIN AppDebug.dbo.ApplicationCodebase acb ON d.CodebaseID = acb.CodebaseID
GROUP BY MetricDate,acb.CodeBaseName
ORDER BY 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="MetricDate">
          <DataField>MetricDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Codebase">
          <DataField>Codebase</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MetricValue">
          <DataField>MetricValue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
