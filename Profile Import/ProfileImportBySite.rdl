  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>MARP</DataSourceName>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
SET NOCOUNT ON

IF OBJECT_ID('tempdb..#ProfileImportUsers') IS NOT NULL
	DROP TABLE #ProfileImportUsers
	
SELECT site.Name as SiteName, (CONVERT(Varchar(2), s.StepNumber) + '-' + step.Name) AS Step, mc.Email, mc.RequestID, s.StepNumber
INTO #ProfileImportUsers
FROM [Starter].[dbo].[MigrationConfig] mc WITH (NOLOCK)
JOIN (	SELECT RequestID, MAX(StepNumber) as StepNumber
		FROM [Starter].[dbo].[MigrationStepStatus] mss WITH (NOLOCK)
		WHERE mss.StepStatusTypeID = 3
		GROUP BY RequestID) s ON mc.RequestID = s.RequestID
JOIN [Starter].[dbo].[MigrationStep] step WITH (NOLOCK) ON s.StepNumber = step.StepID
JOIN [Starter].[dbo].[MigrationSite] site WITH (NOLOCK) ON mc.SiteID = site.SiteID
WHERE mc.SiteID &lt;&gt; 6
AND mc.CreateDt &gt;= DATEADD(Day,-1,GetDate())
AND mc.CreateDt &lt; CAST(GETDATE() AS DATE)

SELECT SiteName,
(SELECT COUNT(1)FROM #ProfileImportUsers piu WHERE piu.SiteName = s.SiteName) as [Daily Total],
(SELECT COUNT(1)FROM #ProfileImportUsers piu WHERE StepNumber &gt;= 2 AND piu.SiteName = s.SiteName) as [Not Existing User],
(SELECT COUNT(1)FROM #ProfileImportUsers piu WHERE StepNumber &gt;= 3 AND piu.SiteName = s.SiteName) as [Reg Created],
(SELECT COUNT(1)FROM #ProfileImportUsers piu WHERE StepNumber &gt;= 4 AND piu.SiteName = s.SiteName) as [Profile Created]
FROM #ProfileImportUsers s
GROUP BY SiteName

DROP TABLE #ProfileImportUsers</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
